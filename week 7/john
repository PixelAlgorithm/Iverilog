S_012B7DD8 .scope module, "tb_johnson_counter" "tb_johnson_counter" 2 1;
 .timescale 0 0;
v012BE6F0_0 .var "clk", 0 0;
RS_011240A4 .resolv tri, L_012BE900, L_012BE9B0, L_012BE748, C4<zzz>;
