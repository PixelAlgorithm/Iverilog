    %vpi_call 2 16 "$monitor", "%0t\011%b\011%b\011%b%b%b", $time, v012BE6F0_0, v012BEB10_0, &PV<v012BE958_0, 2, 1>, &PV<v012BE958_0, 1, 1>, &PV<v012BE958_0, 0, 1>;
    %set/v v012BEB10_0, 1, 1;
    %delay 10, 0;
    %set/v v012BEB10_0, 0, 1;
    %delay 120, 0;
    %set/v v012BEB10_0, 1, 1;
    %delay 10, 0;
    %set/v v012BEB10_0, 0, 1;
    %delay 40, 0;
    %vpi_call 2 21 "$finish";
