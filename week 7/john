L_012B87C8 .functor NOT 1, L_012BEB68, C4<0>, C4<0>, C4<0>;
v012B9718_0 .net *"_s1", 0 0, L_012BEB68; 1 drivers
v012B9770_0 .net "clk", 0 0, v012BE6F0_0; 1 drivers
