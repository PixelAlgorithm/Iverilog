 .timescale 0 0;
v012B3A68_0 .net "D", 0 0, L_012BEA60; 1 drivers
v012B2E48_0 .var "Q", 0 0;
v012B2EA0_0 .alias "clk", 0 0, v012B9770_0;
v012B2EF8_0 .alias "rst", 0 0, v012B9820_0;
S_012B7800 .scope module, "dff2" "d_ff" 3 16, 3 1, S_012B7CC8;
 .timescale 0 0;
v012B8538_0 .net "D", 0 0, L_012BECC8; 1 drivers
v012B8770_0 .var "Q", 0 0;
v012B39B8_0 .alias "clk", 0 0, v012B9770_0;
v012B3A10_0 .alias "rst", 0 0, v012B9820_0;
E_012B8D08 .event posedge, v012B39B8_0;
    .scope S_012B7E60;
T_0 ;
    %wait E_012B8D08;
    %load/v 8, v012B96C0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v012B45F8_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v012B45A0_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v012B45F8_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_012B7AA8;
T_1 ;
    %wait E_012B8D08;
    %load/v 8, v012B2EF8_0, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v012B2E48_0, 0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v012B3A68_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v012B2E48_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_012B7800;
T_2 ;
    %wait E_012B8D08;
    %load/v 8, v012B3A10_0, 1;
    %jmp/0xz  T_2.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v012B8770_0, 0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/v 8, v012B8538_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v012B8770_0, 0, 8;
T_2.1 ;
    %jmp T_2;
