    .scope S_012B7DD8;
T_3 ;
    %set/v v012BE6F0_0, 0, 1;
T_3.0 ;
    %delay 5, 0;
    %load/v 8, v012BE6F0_0, 1;
    %inv 8, 1;
    %set/v v012BE6F0_0, 8, 1;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_012B7DD8;
T_4 ;
    %vpi_call 2 13 "$dumpfile", "johnson_counter_3bit.vcd";
