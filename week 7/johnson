    %vpi_call 2 16 "$monitor", "%0t\011%b\011%b\011%b%b%b", $time, v0123D0B0_0, v0123D000_0, &PV<v0123CC38_0, 2, 1>, &PV<v0123CC38_0, 1, 1>, &PV<v0123CC38_0, 0, 1>;
    %set/v v0123D000_0, 1, 1;
    %delay 10, 0;
    %set/v v0123D000_0, 0, 1;
    %delay 120, 0;
    %set/v v0123D000_0, 1, 1;
    %delay 10, 0;
    %set/v v0123D000_0, 0, 1;
    %delay 40, 0;
    %vpi_call 2 21 "$finish";
    %end;
    .thread T_4;
