    .scope S_011D8710;
T_2 ;
    %wait E_011D8D40;
    %load/v 8, v011D4578_0, 1;
    %jmp/0xz  T_2.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v011D8AC8_0, 0, 0;
