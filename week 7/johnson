    .scope S_011D8468;
T_3 ;
    %set/v v0123D0B0_0, 0, 1;
T_3.0 ;
    %delay 5, 0;
    %load/v 8, v0123D0B0_0, 1;
    %inv 8, 1;
    %set/v v0123D0B0_0, 8, 1;
    %jmp T_3.0;
    %end;
