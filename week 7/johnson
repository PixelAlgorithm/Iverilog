v01213F40_0 .net "D", 0 0, L_0123CEF8; 1 drivers
v011D8AC8_0 .var "Q", 0 0;
v011D4520_0 .alias "clk", 0 0, v011D9A70_0;
v011D4578_0 .alias "rst", 0 0, v011D9B20_0;
E_011D8D40 .event posedge, v011D4520_0;
    .scope S_011D82D0;
T_0 ;
    %wait E_011D8D40;
    %load/v 8, v011D1730_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v011D1680_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v011D1628_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v011D1680_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_011D7A50;
T_1 ;
    %wait E_011D8D40;
    %load/v 8, v011D3E50_0, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v011D3DA0_0, 0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v011D45D0_0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v011D3DA0_0, 0, 8;
T_1.1 ;
    %jmp T_1;
