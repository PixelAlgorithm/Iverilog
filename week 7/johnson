v0123CC38_0 .net8 "q", 2 0, RS_012140AC; 3 drivers
v0123D000_0 .var "reset", 0 0;
S_011D7940 .scope module, "uut" "johnson_counter_3bit" 2 5, 3 10, S_011D8468;
 .timescale 0 0;
L_011D8B20 .functor NOT 1, L_0123CCE8, C4<0>, C4<0>, C4<0>;
v011D9A18_0 .net *"_s1", 0 0, L_0123CCE8; 1 drivers
